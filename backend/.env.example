#############################################################
# Backend Environment Example (backend/.env)
# Duplicate this file as backend/.env and fill values (do NOT commit secrets)
#############################################################

# Web server
PORT=8000
WORKERS=2
TIMEOUT=120
DISABLE_LINKEDIN=1

# CORS (comma separated). Use * ONLY for development
CORS_ORIGINS=http://localhost:5173

# OpenRouter (optional AI augmentation)
OPENROUTER_API_KEY=
OPENROUTER_MODELS=qwen/qwen3-coder:free
OPENROUTER_BASE=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_SITE_URL=http://localhost:5173
OPENROUTER_SITE_NAME=StudentPilot

# Gemini fallback (optional)
GOOGLE_API_KEY=

# Firebase Admin (optional â€“ only if backend verifies tokens or accesses Firestore)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
# Put private key as single line with \n escapes when using dashboards
FIREBASE_PRIVATE_KEY=
